{
  "pages": [
    "pages/login/login",
    "pages/calendar/calendar"
  ],
  "subpackages": [
    {
      "root": "pages/todo-list",
      "pages": [
        "todo-list"
      ]
    },
    {
      "root": "pages/add-todo", 
      "pages": [
        "add-todo"
      ]
    },
    {
      "root": "pages/todo-detail",
      "pages": [
        "todo-detail"
      ]
    }
  ],
  "preloadRule": {
    "pages/calendar/calendar": {
      "network": "all",
      "packages": ["pages/add-todo"]
    }
  },
  "lazyCodeLoading": "requiredComponents",
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#6366f1",
    "navigationBarTitleText": "AI待办助理",
    "navigationBarTextStyle": "white",
    "backgroundColor": "#f8fafc"
  },


  "requiredBackgroundModes": ["audio"],
  "networkTimeout": {
    "request": 10000,
    "uploadFile": 10000,
    "downloadFile": 10000
  },
  "debug": true,
  "style": "v2",
  "sitemapLocation": "sitemap.json"
} 